char str[] = "922337203685477582309";
long long int d1 = 0, d2 = 0;
int16_t res1 = s21_sscanf(str, format, &d1);
int16_t res2 = sscanf(str, format, &d2);
ck_assert_int_eq(res1, res2);
ck_assert_int_eq(d1, d2);


